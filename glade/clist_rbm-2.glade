<?xml version="1.0"?>
<glade-interface>
  <!-- interface-requires gtk+ 2.8 -->
  <!-- interface-naming-policy toplevel-contextual -->
  <widget class="GtkMenu" id="clist_rbm">
    <property name="visible">True</property>
    <child>
      <widget class="GtkImageMenuItem" id="set_as_working_dir1">
        <property name="label" translatable="yes">Set As Working _Folder</property>
        <property name="visible">True</property>
        <property name="tooltip" translatable="yes">Set the current directory as working directory whenever TiLP starts.</property>
        <property name="use_underline">True</property>
        <property name="use_stock">False</property>
        <signal name="activate" handler="rbm_set_as_working_dir1_activate"/>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separator5">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="cut1">
        <property name="label">gtk-cut</property>
        <property name="visible">True</property>
        <property name="tooltip" translatable="yes">Cut selected files</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="rbm_cut1_activate"/>
        <accelerator key="X" signal="activate" modifiers="GDK_CONTROL_MASK"/>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="copy1">
        <property name="label">gtk-copy</property>
        <property name="visible">True</property>
        <property name="tooltip" translatable="yes">Copy selected files</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="rbm_copy1_activate"/>
        <accelerator key="C" signal="activate" modifiers="GDK_CONTROL_MASK"/>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="paste1">
        <property name="label">gtk-paste</property>
        <property name="visible">True</property>
        <property name="tooltip" translatable="yes">Paste previously selected files</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="rbm_paste1_activate"/>
        <accelerator key="V" signal="activate" modifiers="GDK_CONTROL_MASK"/>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="delete_file1">
        <property name="label">gtk-delete</property>
        <property name="visible">True</property>
        <property name="tooltip" translatable="yes">Delete selected file(s)</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="rbm_delete_file1_activate"/>
        <accelerator key="Delete" signal="activate"/>
      </widget>
    </child>
    <child>
      <widget class="GtkMenuItem" id="rename1">
        <property name="visible">True</property>
        <property name="tooltip" translatable="yes">Rename file(s)</property>
        <property name="label" translatable="yes">_Rename</property>
        <property name="use_underline">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separatormenuitem1">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="gfm1">
        <property name="label" translatable="yes">Open With _GFM</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">False</property>
        <signal name="activate" handler="rbm_opengfm_activate"/>
        <child internal-child="image">
          <widget class="GtkImage" id="image2">
            <property name="visible">True</property>
            <property name="stock">gtk-open</property>
          </widget>
        </child>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separator7">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="options1">
        <property name="label">gtk-preferences</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <child>
          <widget class="GtkMenu" id="options1_menu">
            <child>
              <widget class="GtkCheckMenuItem" id="show_all_files1">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_Show all files</property>
                <property name="use_underline">True</property>
                <signal name="activate" handler="rbm_show_all_files1_activate"/>
              </widget>
            </child>
            <child>
              <widget class="GtkCheckMenuItem" id="confirm1">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_Confirm delete/overwrite</property>
                <property name="use_underline">True</property>
                <signal name="activate" handler="rbm_confirm_delete_activate"/>
              </widget>
            </child>
          </widget>
        </child>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separator6">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="properties1">
        <property name="label">gtk-properties</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="rbm_properties1_activate"/>
      </widget>
    </child>
  </widget>
</glade-interface>
