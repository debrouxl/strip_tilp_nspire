<?xml version="1.0"?>
<glade-interface>
  <!-- interface-requires gtk+ 2.8 -->
  <!-- interface-naming-policy toplevel-contextual -->
  <widget class="GtkMenu" id="ctree_rbm">
    <property name="visible">True</property>
    <signal name="key_press_event" handler="on_treeview1_key_press_event"/>
    <child>
      <widget class="GtkImageMenuItem" id="change_device1">
        <property name="label" translatable="yes">Change _Device</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">False</property>
        <signal name="activate" handler="rbm_change_device1_activate"/>
        <accelerator key="D" signal="activate" modifiers="GDK_CONTROL_MASK"/>
        <child internal-child="image">
          <widget class="GtkImage" id="image1">
            <property name="visible">True</property>
            <property name="xalign">0.49000000953674316</property>
            <property name="stock">gtk-connect</property>
          </widget>
        </child>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separator2">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkCheckMenuItem" id="local_view1">
        <property name="visible">True</property>
        <property name="label" translatable="yes">_Local view</property>
        <property name="use_underline">True</property>
        <property name="active">True</property>
        <signal name="activate" handler="rbm_local_view1_activate"/>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separator1">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkCheckMenuItem" id="recv_as_group1">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Recv as _group</property>
        <property name="use_underline">True</property>
        <property name="active">True</property>
        <signal name="activate" handler="rbm_recv_as_group1_activate"/>
      </widget>
    </child>
    <child>
      <widget class="GtkCheckMenuItem" id="backup_as_tigroup1">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Backup as TiGroup</property>
        <property name="use_underline">True</property>
        <property name="active">True</property>
        <signal name="activate" handler="rbm_backup_as_tigroup1_activate"/>
      </widget>
    </child>
    <child>
      <widget class="GtkSeparatorMenuItem" id="separator3">
        <property name="visible">True</property>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="delete_var1">
        <property name="label">gtk-delete</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="rbm_delete_var1_activate"/>
        <accelerator key="Delete" signal="activate"/>
      </widget>
    </child>
    <child>
      <widget class="GtkImageMenuItem" id="create_folder1">
        <property name="label" translatable="yes">Create _Folder</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">False</property>
        <signal name="activate" handler="rbm_create_folder1_activate"/>
        <accelerator key="Insert" signal="activate"/>
        <child internal-child="image">
          <widget class="GtkImage" id="image2">
            <property name="visible">True</property>
            <property name="icon_name">folder-new</property>
          </widget>
        </child>
      </widget>
    </child>
  </widget>
</glade-interface>
